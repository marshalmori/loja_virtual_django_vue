KAAL,CAAW,gBAAX,EAA6B,IAA7B;AACA,WAAK,SAAL;AACD;AArBM;AAVI,CAAf","sourcesContent":["<template>\n  <tr>\n    <td>\n      <router-link :to=\"item.product.get_absolute_url\">{{\n        item.product.name\n      }}</router-link>\n    </td>\n    <td>R$ {{ item.product.price }}</td>\n    <td>\n      {{ item.quantity }}\n      <a @click=\"decrementQuantity(item)\">-</a>\n      <a @click=\"incrementQuantity(item)\">+</a>\n    </td>\n    <td>R$ {{ getItemTotal(item).toFixed(2) }}</td>\n    <td><button class=\"delete\" @click=\"removeFromCart(item)\"></button></td>\n  </tr>\n</template>\n\n<script>\nexport default {\n  name: \"CartItem\",\n  props: {\n    initialItem: Object,\n  },\n  data() {\n    return {\n      item: this.initialItem,\n    };\n  },\n  methods: {\n    getItemTotal(item) {\n      return item.quantity * item.product.price;\n    },\n    decrementQuantity(item) {\n      item.quantity -= 1;\n      if (item.quantity === 0) {\n        this.$emit(\"removeFromCart\", item);\n      }\n      this.updteCart();\n    },\n    incrementQuantity(item) {\n      item.quantity += 1;\n      this.updteCart();\n    },\n    updteCart() {\n      localStorage.setItem(\"cart\", JSON.stringify(this.$store.state.cart));\n    },\n    removeFromCart(item) {\n      this.$emit(\"removeFromCart\", item);\n      this.updteCart();\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}